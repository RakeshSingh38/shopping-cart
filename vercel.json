{
  "rewrites": [
    {
      "source": "/api/products",
      "destination": "/api/products"
    },
    {
      "source": "/api/checkout",
      "destination": "/api/checkout"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}